@model IEnumerable<Project>

@{
    ViewBag.Title = "My Projects";
}
<h1>Shared Projects:</h1>
<div class="card-deck mt-1 ml-2 mr-2">
    @foreach (var project in Model)
    {
        <div class="card ">
            <div class="card-header justify-content-center text-center">
                <h3>@project.Title</h3>
                <h3>@project.Description</h3>
            </div>
            <div class="card-footer text-center">
                @*<a asp-controller="project" asp-action="projectdetails" asp-route-id="@project.ProjectId" class="btn btn-primary">View Details</a>*@
                <a asp-controller="project" asp-action="projectdata" asp-route-id="@project.ProjectId" class="btn btn-primary">View Data</a>               
            </div>
        </div>
    }
</div>
